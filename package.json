{
  "name": "notamud",
  "version": "0.0.1",
  "description": "A MUD for Node and Websockets",
  "main": "./server/index.js",
  "scripts": {
    "build": "npm run-script clean && npm run-script build-server && npm run-script copy-data",
    "clean": "rimraf ./server",
    "build-server": "tsc --project ./src/server/tsconfig.json",
    "copy-data": "copyfiles -e ./**/tsconfig.json -u 2 ./src/server/**/*.json ./server",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RonPenton/NotaMUD.git"
  },
  "keywords": [
    "MUD"
  ],
  "author": "Ron Penton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RonPenton/NotaMUD/issues"
  },
  "homepage": "https://github.com/RonPenton/NotaMUD#readme",
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.22",
    "@types/socket.io": "^1.4.29",
    "copyfiles": "^1.2.0",
    "express": "^4.15.4",
    "rimraf": "^2.6.1",
    "socket.io": "^2.0.3",
    "typescript": "^2.4.2"
  }
}
