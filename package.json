{
    "name": "notamud",
    "version": "0.1.0",
    "description": "A MUD for Node and Websockets",
    "main": "./server/index.js",
    "scripts": {
        "start": "npm run build && npm run-script go",
        "go": "node ./build/index.js",
        "init-db": "npm run build && node ./build/setup/index.js",
        "build": "npm run-script clean && npm run-script tools && npm run-script build-server && npm run-script build-client && npm run-script copy-data",
        "tools": "npm run-script build-tools && npm run-script run-tools",
        "clean": "rimraf ./build && rimraf ./build-tools",
        "build-server": "tsc --project ./src/server/tsconfig.json",
        "build-client": "webpack --config ./src/client/webpack.config.js",
        "build-tools": "tsc --project ./src/tools/tsconfig.json",
        "run-tools": "",
        "copy-data": "npm run-script copy-foundation && npm run-script copy-css && npm run-script copy-ejs && npm run-script copy-svg && npm run-script copy-require",
        "copy-foundation": "copyfiles -u 4 ./node_modules/foundation-sites/dist/css/foundation.min.css ./build/public",
        "copy-css": "copyfiles -u 2 ./src/client/**/*.css ./build/public",
        "copy-ejs": "copyfiles -u 3 ./src/client/views/**/*.ejs ./build/views",
        "copy-svg": "copyfiles -u 2 ./src/client/**/*.svg ./build/public",
        "copy-require": "copyfiles -u 2 ./node_modules/requirejs/require.js ./build/public",
        "copy-fast": "npm run-script copy-css && npm run-script copy-ejs",
        "build-fast": "npm run-script build-client && npm run-script copy-fast",
        "aws-start": "forever start ./build/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/RonPenton/NotaMUD.git"
    },
    "keywords": [
        "MUD"
    ],
    "author": "Ron Penton",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/RonPenton/NotaMUD/issues"
    },
    "homepage": "https://github.com/RonPenton/NotaMUD#readme",
    "dependencies": {
        "@types/babel-types": "^7.0.8",
        "@types/babylon": "^6.16.5",
        "@types/bcrypt-nodejs": "0.0.31",
        "@types/body-parser": "^1.19.0",
        "@types/connect-flash": "0.0.35",
        "@types/cookie-parser": "^1.4.2",
        "@types/deep-freeze": "^0.1.2",
        "@types/express": "^4.17.7",
        "@types/express-session": "^1.17.0",
        "@types/lodash": "^4.14.158",
        "@types/morgan": "^1.9.1",
        "@types/node": "^14.0.26",
        "@types/passport": "^1.0.4",
        "@types/passport-local": "^1.0.33",
        "@types/ramda": "^0.27.12",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/socket.io": "^2.1.10",
        "@types/socket.io-client": "^1.4.33",
        "aws-sdk": "^2.721.0",
        "babel-types": "^6.26.0",
        "babylon": "^6.18.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.19.0",
        "connect-dynamodb": "^2.0.4",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.5",
        "copyfiles": "^2.3.0",
        "css-loader": "^4.0.0",
        "decko": "^1.2.0",
        "deep-freeze": "0.0.1",
        "dexie": "^3.0.1",
        "ejs": "^3.1.3",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "foundation-sites": "^6.6.3",
        "lodash": "^4.17.19",
        "moment": "^2.27.0",
        "morgan": "^1.10.0",
        "node-sass": "^4.14.1",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport.socketio": "^3.7.0",
        "ramda": "^0.27.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-foundation": "^0.9.6",
        "requirejs": "^2.3.6",
        "rimraf": "^3.0.2",
        "sass-loader": "^9.0.2",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "style-loader": "^1.2.1",
        "ts-keycode-enum": "^1.0.6",
        "ts-loader": "^8.0.1",
        "ts-priority-queue": "^0.1.1",
        "tslint": "^6.1.2",
        "typescript": "^3.9.7",
        "webpack": "^4.44.0"
    },
    "devDependencies": {
        "mini-css-extract-plugin": "^0.9.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "webpack-cli": "^3.3.12"
    }
}
